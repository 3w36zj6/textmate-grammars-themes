{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "description": "Collection of TextMate grammars and themes in JSON",
  "scripts": {
    "build": "pnpm -r build",
    "stub": "pnpm -r stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "bump": "esno scripts/bump.ts",
    "test": "vitest",
    "fetch": "esno scripts/fetch.ts && eslint . --fix",
    "fetch:force": "esno scripts/fetch.ts --force && eslint . --fix",
    "typecheck": "tsc --noEmit",
    "play": "nr -C playground dev",
    "play:build": "nr -C playground build",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/adm-zip": "^0.5.5",
    "@types/cson": "^7.20.3",
    "@types/js-yaml": "^4.0.9",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/node": "^20.11.10",
    "@types/semver": "^7.5.6",
    "adm-zip": "^0.5.10",
    "bumpp": "^9.3.0",
    "chalk": "^5.3.0",
    "cson": "^8.4.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "fast-plist": "^0.1.3",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.1.1",
    "jsonc-parser": "^3.2.1",
    "lint-staged": "^15.2.0",
    "octokit": "^3.1.2",
    "ofetch": "^1.3.3",
    "p-limit": "^5.0.0",
    "pnpm": "^8.15.1",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "simple-git": "^3.22.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27",
    "yaml": "^2.3.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
